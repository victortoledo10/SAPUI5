
<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <List
      headerText="{i18n>invoiceListTitle}"
      class="sapUiResponsiveMargin"
      width="auto"
      items="{data>/Invoices}" >
      <items>
         <ObjectListItem
            title="{data>Quantity} x {data>ProductName}"/>
      </items>
   </List>
</mvc:View>


